<div id="<%= dom_id batch %>">
      <div class="card mb-3">
        <div class="card-header">Batch</div>
        <div class="card-body">
            <p>
              <strong>Title:</strong>
              <%= batch.title %>
            </p>
            <p>
              <strong>Branch:</strong>
              <%= batch.branch %>
            </p>
            <p>
              <%= batch.description %>
            </p>
        </div>
      </div>
<div class="mb-3">
  <%= link_to '<i class="bi bi-arrow-bar-left"></i>'.html_safe, batches_path, class: "btn btn-secondary" %>
  <%= link_to "New Loan", new_coverage_path(b: batch), class: "btn btn-primary", data: { turbo_frame: "remote_modal" }%>
  <%= link_to 'All', batch_url(batch, s: 0), class: 'btn btn-secondary btn-sm' %>
  <%= link_to '18 - 65', batch_url(batch, s: 1), class: 'btn btn-secondary btn-sm' %>
</div>
<%#= link_to 'Add Branch', new_coop_branch_path(v: c) , class: 'btn btn-primary', data: { turbo_frame: "remote_modal" } %>
<%= render 'coverages/show_loan_coverage', sw: @show_coverage %>
</div>