<table class="table table-striped table-bordered table-hover">
  <thead>
      <tr>
      <th>#</th>
            <th>Member Type</th>
            <th>Term</th>
            <th>Residency Floor</th>
            <th>Residency Ceiling</th>
            <th>Premium</th>
            <th></th>
        </tr>
  </thead>
  <tbody>
    <% sw.each do |gp| %>
      <tr>
        <th scope="row"><%= gp.id %></th>
        <td><%= gp.member_type %></td>
        <td><%= gp.term %></td>
        <td><%= gp.residency_floor %></td>
        <td><%= gp.residency_ceiling %></td>
        <td><%= gp.premium %></td>
        <td>
          <%= link_to '<i class="bi bi-eye-fill"></i>'.html_safe, gp, class: 'btn btn-secondary btn-sm' %>
          <%= link_to '<i class="bi bi-pencil-square"></i>'.html_safe, edit_group_premium_path(gp), class: 'btn btn-secondary btn-sm' %>
          <%= link_to '<i class="bi bi-trash"></i>'.html_safe, gp, class: "btn btn-danger btn-sm", method: :delete, data: { "turbo-method": :delete, turbo_confirm: "Are you sure?" } %>
        </td>
      </tr>
    <% end %>

  </tbody>
</table>